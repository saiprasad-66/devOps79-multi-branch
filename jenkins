pipeline{
   agent any
   stages{
   stage{'sprint1 branch'} {
   steps{
    sh 'echo "this is sprint1 branch"
     }
    }
     stage{'testing'} {
   steps{
    sh 'echo "this is testing application"
     }
    }
 stage{'deploy application'} {
   steps{
    sh 'echo "this is deploying application"
     }
    }
   }
 }

